{
  "$schema": "https://playground.wordpress.net/blueprint-schema.json",
  "landingPage": "/wp-admin/admin.php?page=wc-settings&tab=wc_simple_buy_settings",
  "preferredVersions": {
    "php": "8.3",
    "wp": "latest"
  },
  "phpExtensionBundles": [
    "kitchen-sink"
  ],
  "features": {
    "networking": true
  },
  "steps": [
    {
      "step": "installPlugin",
      "pluginZipFile": {
        "resource": "wordpress.org/plugins",
        "slug": "woocommerce"
      },
      "options": {
        "activate": true
      }
    },
    {
      "step": "installPlugin",
      "pluginData": {
        "resource": "wordpress.org/plugins",
        "slug": "buy-now-woo"
      },
      "options": {
        "activate": true
      }
    },
    {
      "step": "installTheme",
      "themeData": {
        "resource": "wordpress.org/themes",
        "slug": "storefront"
      },
      "options": {
        "activate": true
      }
    },
    {
      "step": "setSiteOptions",
      "options": {
        "woocommerce_onboarding_profile": "a:1:{s:7:\"skipped\";b:1;}",
        "woocommerce_currency": "BDT",
        "woocommerce_default_country": "BD:BD-13"
      }
    },
    {
      "step": "importWxr",
      "file": {
        "resource": "url",
        "url": "https://gist.githubusercontent.com/Niloys7/9b82e0f2331e26fdf44f98be13f00331/raw/611128ef3eb87464f34067f316ba26468f2a9867/sample_products.xml"
      }
    },
    {
      "step": "login",
      "username": "admin",
      "password": "password"
    }
  ]
}